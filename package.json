    "apollo": "^2.21.3",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "apollo-link-schema": "^1.2.4",
    "autocannon": "^4.5.2",
    "color-space": "^1.16.0",
    "fast-sha256": "^1.3.0",
    "file-loader": "^5.0.2",
    "fp-ts": "^2.4.3",
    "get-graphql-schema": "^2.1.2",
    "graphiql": "^0.17.5",
    "graphql": "^14.6.0",
    "graphql-cli": "^3.0.14",
    "graphql-docs": "^0.2.0",
