    "jq.node": "^2.1.2",
    "next": "^9.2.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^7.1.3",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "generate": "bash scripts/generate.bash",
    "format": "bash scripts/format.bash",
    "start": "bash scripts/start.bash",
    "export": "bash scripts/export.bash",
    "benchmark": "bash scripts/benchmark.bash",
    "fix": "bash scripts/fix.bash",
    "build": "cp -f node_modules/graphql-voyager/dist/voyager.worker.js public/graphql/voyager.worker.js && cp -f node_modules/graphql-voyager/dist/voyager.css public/graphql/voyager.css && next build",
    "lint": "bash scripts/lint.bash",
    "precommit": "yarn install && yarn format && echo 'grep -q precommit package.json && (yarn precommit && git add .) || echo' > .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "gl": "git fetch --all; git log --decorate --graph --first-parent --all  --format=short -m --stat -- . ':!*.lock' ':!Cargo.toml' ':!package.json' ':!.*'; git config alias.l '!yarn gl'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "firefox esr",
      "last 1 firefox version",
      "last 1 edge version",
      "last 1 ios version",
      "last 1 android version",
      "last 1 chrome version"
    ],
    "development": [
      "last 1 firefox version",
      "last 1 chrome version"
    ]
  },
  "files": [
    "src/pages/**/*",
    "src/pages-lib/**/*",
    "public/**/*"
  ]
}
